{
    "name": "apify-store-scraper",
    "version": "1.0.0",
    "type": "module",
    "description": "Extract data about actors from Apify Store.",
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "@algolia/client-search": "^5.46.2",
        "@crawlee/http": "^3.15.3",
        "apify": "^3.5.1",
        "cheerio": "^1.0.0-rc.12",
        "crawlee": "^3.15.2"
    },
    "devDependencies": {
        "@apify/eslint-config-ts": "^0.3.0",
        "@apify/tsconfig": "^0.1.0",
        "@babel/core": "^7.24.5",
        "@babel/preset-env": "^7.24.5",
        "@babel/preset-typescript": "^7.24.1",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^6.7.2",
        "@typescript-eslint/parser": "^6.7.2",
        "babel-jest": "^29.7.0",
        "esbuild": "0.25.0",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "jest": "^29.7.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2"
    },
    "scripts": {
        "start": "npm run start:dev",
        "start:prod": "node dist/src/main.js",
        "start:dev": "set -a; . ./.env; set +a; node --inspect --no-warnings=ExperimentalWarning --import tsx src/main.ts",
        "build": "esbuild ./src/main.ts --bundle --platform=node --format=esm --packages=external --outfile=./dist/src/main.js",
        "lint": "eslint ./src --ext .ts",
        "lint:fix": "eslint ./src --ext .ts --fix",
        "test": "npx jest"
    },
    "license": "ISC"
}
